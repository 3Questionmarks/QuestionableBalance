<GameData>
	<Defines>
		<!-- Cities can expand out to 12 tiles -->
		<Update>
			<Set Value="12"/>
			<Where Name="MAXIMUM_ACQUIRE_PLOT_DISTANCE"/>
		</Update>
		<!-- make tiles cheaper to buy -->
		<Update>
			<Where Name="PLOT_BASE_COST"/>
			<Set Value="50" />
		</Update>
		<Update>
			<Where Name="PLOT_ADDITIONAL_COST_PER_PLOT"/>
			<Set Value="0" />
		</Update>
	</Defines>
</GameData>